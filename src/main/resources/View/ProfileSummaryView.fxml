            <HBox prefHeight="206.0" prefWidth="600.0">
               <children>
                  <ListView fx:id="characterListView" prefHeight="206.0" prefWidth="207.0" />
               </children>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </HBox>
         </children></VBox>
